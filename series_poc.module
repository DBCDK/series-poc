/**
 * Implements hook_ctools_plugin_directory().
 *
 * It simply tells panels where to find the .inc files that define various
 * args, contexts, content_types.
 */
function series_poc_ctools_plugin_directory($module, $plugin)
{
  if ($module == 'ctools' && !empty($plugin)) {
    return "plugins/$plugin";
  }
}

function series_poc_search()
{
  $category = '';
  $parameters = drupal_get_query_parameters();
  $select = '';

  if (isset($parameters['series'])) {
    $series = series_poc_get_works_in_series($parameters['series']);
    $items = series_poc_handle_series($series);
    $menu = series_poc_handle_menu_series($series);
    $bread_crumb = series_poc_get_breadcrumb($series);
  } else if (isset($parameters['universe'])) {
    $universe = series_poc_get_universe_with_series($parameters['universe']);
    $items = series_poc_handle_universe($universe);
    $menu = series_poc_handle_menu_universe($universe);
    $universe->universe_title = $universe->title;
    $bread_crumb = series_poc_get_breadcrumb($universe, true);
    //file_put_contents("/var/www/drupalvm/drupal/web/debug/uni1.txt", print_r($universe, TRUE), FILE_APPEND);
  } else {
    if (isset($parameters['show']) && $parameters['show'] == 'universe') {
      $all_universes = series_poc_get_all_series('universe');
      $items = series_poc_show_all_universes($all_universes);
      $menu = series_poc_handle_menu_categories();
      $bread_crumb = series_poc_get_breadcrumb();
      $select = series_poc_get_select(true);
      //file_put_contents("/var/www/drupalvm/drupal/web/debug/universe1.txt", print_r($all_universes, TRUE), FILE_APPEND);
    } else {
      $all_series = series_poc_get_all_series('series');
      $items = series_poc_show_all_series($all_series);
      $menu = series_poc_handle_menu_categories();
      $bread_crumb = series_poc_get_breadcrumb();
      $select = series_poc_get_select(false);
      file_put_contents("/var/www/drupalvm/drupal/web/debug/series1.txt", print_r($all_series, TRUE), FILE_APPEND);
    }
  }


  drupal_add_js(drupal_get_path('module', 'ting_covers') . '/js/ting-covers.js');
  return theme('series_poc_page', array('items' => $items, 'breadcrumb' => $bread_crumb, 'menu' => $menu, 'select' => $select));
}

function series_poc_get_select($is_universe)
{
  $links = array();
  $parameters = drupal_get_query_parameters();
  $options = array(
    'html' => TRUE,
  );
  if (isset($parameters['category'])) {
    $options['query'] = array('category' => $parameters['category']);
  }
  $universe_options = $options;
  $universe_options['query']['show'] = 'universe';
  if ($is_universe) {
    $universe_options['attributes'] = array('class' => array('series_poc-select-item-selected'));
  } else {
    $options['attributes'] = array('class' => array('series_poc-select-item-selected'));
  }
  $links[] = l('Serier', '/serier', $options);
  $links[] = l('Universer', '/serier', $universe_options);
  return implode('', $links);
}

function series_poc_get_breadcrumb($series = null, $is_universe = false)
{
  $links = array();
  $links[] = l('Serier', '/serier');
  if (!isset($series)) {
    $parameters = drupal_get_query_parameters();

    if (isset($parameters['category'])) {
      $options = array(
        'html' => TRUE,
        'query' => array('category' => $parameters['category']),
      );
      $links[] = l($parameters['category'], '/serier', $options);
    }
  } else {
    $category = series_poc_get_category_series($series);
    $options = array(
      'html' => TRUE,
      'query' => array('category' => $category),
    );
    $links[] = l(ucFirst($category), '/serier', $options);
    if (isset($series->universe_title)) {
      $options = array(
        'html' => TRUE,
        'query' => array('universe' => $series->universe_title),
      );
      $links[] = l($series->universe_title, '/serier', $options);
    }
    //file_put_contents("/var/www/drupalvm/drupal/web/debug/menu1.txt", print_r($series, TRUE), FILE_APPEND);
    if (!$is_universe) {
      $options = array(
        'html' => TRUE,
        'query' => array('series' => $series->title),
      );
      $links[] = l($series->title, '/serier', $options);
    }
  }
  return implode('<span class="series_poc-breadcrumb-icon"></span>', $links);
}

function series_poc_handle_menu_universe($universe)
{
  $category = series_poc_get_category_series($universe);
  $series_item = '';
  foreach ($universe->series as $series) {
    $series_item .= series_poc_make_series_menu_item($series->title, 'series', false);
  }
  $series_menu_item = '<ul class="series-menu-sub-level-2">' . $series_item . '</ul>';
  //file_put_contents("/var/www/drupalvm/drupal/web/debug/menu10.txt", print_r($series_menu_item, TRUE), FILE_APPEND);

  $univers_menu_item = '<ul class="series-menu-sub-level">' . series_poc_make_series_menu_item($universe->title, 'universe', true, $series_menu_item) . '</ul>';
  return series_poc_handle_menu($category, $univers_menu_item);
}


function series_poc_handle_menu_series($series)
{
  $category = series_poc_get_category_series($series);
  if (isset($series->universe_title)) {
    $universe = series_poc_get_universe($series->universe_title);
    //file_put_contents("/var/www/drupalvm/drupal/web/debug/menu2.txt", print_r($universe, TRUE), FILE_APPEND);
    $series_menu_item = '<ul class="series-menu-sub-level-2">' ;
    foreach ($universe->included_series as $universe_series) {
      if ($series->title == $universe_series) {
        $series_menu_item .= series_poc_make_series_menu_item($universe_series, 'series', true);
      } else {
        $series_menu_item .= series_poc_make_series_menu_item($universe_series, 'series', false);
      }
    }
    $series_menu_item .= '</ul>';
    $univers_menu_item = '<ul class="series-menu-sub-level">' . series_poc_make_series_menu_item($series->universe_title, 'universe', false, $series_menu_item) . '</ul>';
    return series_poc_handle_menu($category, $univers_menu_item);
  } else {
    $series_menu_item = '<ul class="series-menu-sub-level series-menu-sub-level-active">' . series_poc_make_series_menu_item($series->title, 'series', true) . '</ul>';
    return series_poc_handle_menu($category, $series_menu_item);
  }
}

function series_poc_get_category_series($series)
{
  foreach (series_poc_get_category_data() as $key => $title) {
    if ($key == $series->title) {
      return $title['category'];
    }
  }
}

function series_poc_handle_menu_categories()
{
  $parameters = drupal_get_query_parameters();

  if (isset($parameters['category'])) {
    $category = $parameters['category'];
    return series_poc_handle_menu($category);
  } else {
    return series_poc_handle_menu();
  }
}

function series_poc_handle_menu($category = '', $subitems = '')
{
  $menu_items = [];
  $items = [
    'romaner' => 'serier?category=romaner',
    'krimi' => 'serier?category=krimi',
    'fantasy' => 'serier?category=fantasy',
    'fag' => 'serier?category=fag',
    'børn' => 'serier?category=børm',
  ];

  foreach ($items as $key => $item) {
    if ($key == $category) {
      $menu_items[] = series_poc_make_menu_item($item, $key, true, $subitems);
    } else {
      $menu_items[] = series_poc_make_menu_item($item, $key);
    }
  }
  return theme('series_poc_menu', array('items' => $menu_items));
}

function series_poc_make_series_menu_item($title, $type, $active = false, $subitems = '')
{
  $class = 'series-menu-item';
  if ($active) {
    $class .= ' series-menu-item-active';
  }
  $options = array(
    'html' => TRUE,
    'query' => array($type => $title),
  );
  $link = l($title, 'serier', $options);

  return '<li class="' . $class . '">' . $link . $subitems  . '</li>';
}

function series_poc_make_menu_item($item, $key, $active = false, $subitems = '')
{
  $class = 'series-menu-item';
  if ($active) {
    $class .= ' series-menu-item-active';
    if ($subitems == '') {
      $class .= ' series-menu-item-selected';
    }
  }
  $options = array(
    'html' => TRUE,
    'query' => array('category' => $key),
  );
  $link = l(ucfirst($key), 'serier', $options);

  return '<li class="' . $class . '">' . $link . $subitems . '</li>';
}

function series_poc_show_all_series($series)
{
  $items = [];
  if (isset($series)) {
    foreach ($series as $series_item) {
      if (!empty($series_item)) {
        $items[] = series_poc_show_all_series_item($series_item);
      }
    }
  }
  return theme('series_poc_main_all', array('items' => $items));
}

