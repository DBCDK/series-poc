{
  //file_put_contents("/var/www/drupalvm/drupal/web/debug/universe3.txt", print_r($universes, TRUE), FILE_APPEND);
  $items = [];
  if (isset($universes)) {
    foreach ($universes as $series_item) {
      if (!empty($series_item)) {
